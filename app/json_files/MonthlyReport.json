					"type": "column",
					"average": true,
					"slantedText": true,
					"src": "sql",
					"sql": "select 'month_'||month as 'Month',count(1) as 'Number of Events' from attacks where year={year}-1 or month<={month} group by month order by year"
				},
				{
					"object": "chart",
					"style": {
						"top": 850,
						"width": "100%"
					},
					"centered": true,
					"total": true,
					"average": true,
					"type": "table",
					"src": "last"
				}
			]
		},
