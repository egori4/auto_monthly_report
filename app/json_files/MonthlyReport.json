{
	"offline": true,
	"title": "Monthly Report - {month} {year}",
	"defaults": {
		"chart": {
			"db": "database_files/{id}/database_{id}.sqlite"
		},
		"variable": {
			"db": "database_files/{id}/database_{id}.sqlite"
		},
		"page": {
			"objects": [
				{
					"object": "image",
					"style": {
						"top": 0,
						"left": 0
					},
					"src": "{relPath}html_files/images/header.jpg",
					"width": 1100,
					"height": 100
				},
				{
					"object": "image",
					"style": {
						"bottom": 0,
						"left": 0
					},
					"src": "{relPath}html_files/images/footer.jpg",
					"width": 1100,
					"height": 100
				},
				{
					"object": "text",
					"style": {
						"top": 25,
						"right": 40,
						"width": 700,
						"height": 200,
						"color": "white",
						"font-size": 36,
						"text-align": "right"
					},
					"text": "Monthly Report - {monthText} {year}"
				},
				{
					"object": "text",
					"style": {
						"bottom": 120,
						"right": 40
					},
					"text": "<a href='#index'>Back to Index</a>"
				},
				{
					"object": "text",
					"style": {
						"bottom": 40,
						"left": 40,
						"color": "white"
					},
					"text": "Report v{version}"
				},
				{
					"object": "text",
					"style": {
						"bottom": 40,
						"right": 40,
						"color": "white"
					},
					"text": "Page: {pageNumber}"
				}
			]
		}
	},
	"pages": [
		{
			"title": "Overview",
			"id": "overview",
			"objects": [
				{
					"object": "text",
					"style": {
						"top": 230,
						"left": 0,
						"width": "100%",
						"height": 50,
						"font-size": 42,
						"text-align": "center"
					},
					"text": "Monthly Report - {monthText} {year}"
				},
				{
					"object": "text",
					"style": {
						"top": 330,
						"left": 0,
						"width": "100%",
						"height": 50,
						"font-size": 42,
						"text-align": "center"
					},
					"text": "{longName}"
				},
				{
					"object": "text",
					"style": {
						"top": 450,
						"left": 200,
						"width": 700,
						"height": 700,
						"font-size": 18
					},
					"src": "file",
					"file": "text_files/frontpage_{language}.txt"
				},
				{
					"object": "text",
					"style": {
						"top": 950,
						"width": "100%",
						"height": 20,
						"font-size": 18,
						"font-weight": "bold"
					},
					"centered": true,
					"text": "Event Statistics Overview"
				},
				{
					"object": "variable",
					"name": "totalRecs",
					"src": "sql",
					"sql": "select count(1) from attacks where month={month}",
					"format": "number",
					"decimals": 0
				},
				{
					"object": "variable",
					"name": "totalMPackets",
					"src": "sql",
					"sql": "select printf('%.{pktDecimals}f', sum(packetCount)*1.0/{pktDivisor}) from attacks where month={month}",
					"format": "number",
					"decimals": "{pktDecimals}"
				},
				{
					"object": "variable",
					"name": "totalGBytes",
					"src": "sql",
					"sql": "select printf('%.{bwDecimals}f', sum(packetBandwidth)*1.0/{bwDivisor}) from attacks where month={month}",
					"format": "number",
					"decimals": "{bwDecimals}"
				},
				{
					"object": "chart",
					"style": {
						"top": 975,
						"width": "100%",
						"height": 400
					},
					"type": "table",
					"fields": [
						"string",
						"string"
					],
					"centered": true,
					"options": {
						"cssClassNames": "{ headerCell: \"noHeader\", tableRow: \"summary\", oddTableRow: \"summary\" }"
					},
					"data": [
						[
							"col1",
							"col2"
						],
						[
							"Total events detected",
							"{totalRecs}"
						],
						[
							"Total number of malicious packets",
							"{totalMPackets} {pktUnit}"
						],
						[
							"Total malicious bandwidth",
							"{totalGBytes} {bwUnit}"
						]						
					]
				}
			]
		},
		{
			"title": "Index",
			"id": "index",
			"objects": [
				{
					"object": "text",
					"style": {
						"top": 150,
						"left": 0,
						"width": "100%",
						"height": 50,
						"font-size": 38,
						"text-align": "center",
						"color": "#113150"
					},
					"text": "Content Index"
				},
				{
					"object": "index",
					"style": {
						"top": 250,
						"width": "100%"
					},
					"centered": true,
					"minPage": 1,
					"maxPage": 35,
					"options": {
						"cssClassNames": "{ tableRow: \"index\", oddTableRow: \"index\" }"
					}
				}
			]
		},
		{
			"title": "Traffic in Megabits",
			"id": "trfmbit",
			"objects": [
				{
					"object": "chart",
					"style": {
						"top": 140,
						"left": 0,
						"width": 1100,
						"height": 680
					},
					"title": "Traffic in Megabits",
					"type": "area",
					"divisor": 1000,
					"noLabel": true,
					"slantedText": true,
					"showTextEvery": 168,
					"src": "sql",
					"sql": "select printf('%s', strftime('{monthText} %d', dateTime)) as 'Date',sum(trafficValue) as 'Inbound Traffic',sum(discards) as 'Discarded Traffic',sum(excluded) as 'Excluded Traffic' from traffic where month={month} group by Date,strftime('%H', dateTime) order by timeStamp"
				},
				{
					"object": "text",
					"style": {
						"top": 800,
						"width": "100%"
					},
					"centered": true,
					"text": "<table><tr><td>. Discarded Traffic is traffic detected as malicious and dropped by the device.<br>. Excluded Traffic is traffic which doesn't match any network class.<br>. Excluded Traffic traverses the device and is only checked for packet anomalies.</td></tr></table>"
				}
			]
		},
		{
			"title": "Events per Day of the Month",
			"id": "evtday",
			"objects": [
				{
					"object": "chart",
					"style": {
						"top": 140,
						"left": 0,
						"width": 1100,
						"height": 580
					},
					"title": "Events per Day of the Month",
					"type": "area",
					"noLabel": true,
					"legend": "none",
					"average": true,
					"labelAsAlpha": true,
					"xMin": 0,
					"xMax": "{monthDays}",
					"src": "sql",
					"sql": "select x as 'Day of the Month', sum(y) as 'Number of Events' from (with recursive cnt(x) as (select 1 union select x+1 from cnt limit {monthDays}) select x ,0 as 'y' from cnt union select endDayOfMonth,count(1) from attacks where month={month} group by endDayOfMonth) group by x order by x"
				},
				{
					"object": "chart",
					"style": {
						"top": 645,
						"width": "100%"
					},
					"centered": true,
					"average": true,
					"type": "table",
					"src": "last"
				}
			]
		},
		{
			"title": "Total Events Count (Month to Month)",
			"id": "evtmon",
